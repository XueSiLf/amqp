{
    "name": "lavamusic/amqp",
    "type": "library",
    "license": [
        "Apache-2.0"
    ],
    "description": "支持在 EasySwoole 框架中使用 支持 AMQP 协议的消息队列，如：RabbitMQ",
    "keywords": ["easyswoole","rabbitmq", "message", "queue"],
    "authors": [
        {
            "name": "XueSi",
            "email": "1592328848@qq.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-mbstring": "*",
        "php-amqplib/php-amqplib": "^3.1",
        "easyswoole/spl": "^1.4",
        "easyswoole/swoole-ide-helper": "^1.3"
    },
    "require-dev": {
        "easyswoole/phpunit": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "LavaMusic\\AMQP\\": "src/"
        },
        "files": ["src/Helper/func.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "LavaMusic\\AMQP\\Test\\": "tests/"
        },
        "files": ["src/Helper/func.php"]
    }
}
